Algoritmo ES_09
	Definir precio Como Entero;
	Definir  precioConDescuento Como real;
	
	Mostrar "Ingrese el importe";
	Leer precio;
	
	precioConDescuento <- precio-precio*0.3;
	Mostrar "El precio con descuento es $ ", precioConDescuento;
	
FinAlgoritmo
